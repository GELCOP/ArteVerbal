    <TIER LINGUISTIC_TYPE_REF="Traducción" PARENT_REF="Transcripción"
        PARTICIPANT="Manuel Santana" TIER_ID="Traducción">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a38" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>Antes los yaguas vivían.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a39" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>Los indígenas les mataron a ellos [a los yagua]. Entonces, lo mataron, entonces, dijo:</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a40" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>“¿Dónde esta mi mamá? No hay mi mamá, los indígenas ya los mataron a ellos, a todos ellos”. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a41" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>“Le mataron a ella, que estaba embarazada; estaba embarazada de dos bebe”.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a42" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE>Entonces, preguntaban [los hijos], entonces creció¹, le preguntaba a su abuela:</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a43" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>“¿Dónde esta mi mamá, dónde está mi papá?” Su abuela le decía: “Los indígenas les mataron antes a tu mamá [y] a tu papá”;</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a44" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>“entonces, ahora [que] ya no está ella, andaran conmigo hasta que crescan ustedes”.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a45" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>La abuela no tenía la leche de vaca, para que tomen los niños,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a46" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>con bebidas dulces, le daba [de] tomar.  “[Con] jugo de caña, así, le hice crecer con jugo de caña; tomaban jugo de caña y crecieron”, [decía la abuela].</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a47" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>Entonces, cuando ya era grande [es decir, los hijos] preguntaban.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a48" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>Cuando tenía 10 años, [uno de ellos] le preguntaba a su hermano:</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a49" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>“¿Vamos a preguntar a la abuela dónde está mi papá?, ¿cómo se murieron?”. Entonces, el abuelo les contó:</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a50" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>“Los indígenas mataron a tu mamá. Vamos a matarlos a todos ellos. Yo sí sé cómo les vamos a matar”.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a51" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE>Entonces, [el abuelo] le dijo al hermano: “Transfórmate como un gavilán grande [y]</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a52" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE>vuela encima de su casa. Entonces, mirarán [y dirán] quién es, qué [es] eso tan lindo, qué es eso tan lindo.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a53" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>Cuando salen los indígenas de su casa, entonces, sopla el fijuano de tu papá. [Así], pues morirán.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a54" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE>Entonces, morirán. En ese momento, tu hermano les matará con su mazo”.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a55" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>Entonces, los dos [hermanos] mataron todo. Mataron todo.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a56" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE>Entonces hasta ahí nomás mataron por el pago de la muerte [de sus padres].</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a57" ANNOTATION_REF="a20">
                <ANNOTATION_VALUE>“Ya le hemos matado por la muerte de mamá, les matamos a todos que mataron a mi mamá finada”, [dijo el hermano]. Le dijo: “qué vamos [a] hacer. Ahora [qué] vamos a hacer”.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a58" ANNOTATION_REF="a21">
                <ANNOTATION_VALUE>“En delante de nuestros compañeros nos paramos. Nos paramos en delante de nuestros compañeros. Les pregunto qué, cómo vamos a hacer, vamos a matarlos a todos”, [dijo el hermano].</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a59" ANNOTATION_REF="a22">
                <ANNOTATION_VALUE>Dentro de la tierra,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a60" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE>machacaron el piripri para matar; entonces, mataron a todos.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a61" ANNOTATION_REF="a24">
                <ANNOTATION_VALUE>Cinco mil,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a62" ANNOTATION_REF="a25">
                <ANNOTATION_VALUE>mataron a cinco mil</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a63" ANNOTATION_REF="a26">
                <ANNOTATION_VALUE>mataron, bastante mataron.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a64" ANNOTATION_REF="a27">
                <ANNOTATION_VALUE>Mataban con mazos, no mataban con escopetas, no cortaban con machete, todo con mazo.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a65" ANNOTATION_REF="a28">
                <ANNOTATION_VALUE>Les mataron a todos. De ahí, vino a Pebas. En el fondo de la cabecera de Pebas se murió Ruweju, [uno de los hermanos].</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a66" ANNOTATION_REF="a29">
                <ANNOTATION_VALUE>Allá, entonces, se murió Mokï, [el otro hermano]; su papá, entonces, se llamaba Sunujañu,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a67" ANNOTATION_REF="a30">
                <ANNOTATION_VALUE>se llamaba su papá, así se llamaba su papá. No le conosco a su mamá, no se cómo se llama. Así, entonces eran ellos.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a68" ANNOTATION_REF="a31">
                <ANNOTATION_VALUE>Todos lo[s] admiraban [es decir, a los hermano]. No podían matarle, no moría. Él estaba ahí,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a69" ANNOTATION_REF="a32">
                <ANNOTATION_VALUE>él agarraba las flechas de los indígenas.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a70" ANNOTATION_REF="a33">
                <ANNOTATION_VALUE>Agarraba las flechas que lo picaban, con sus propia flecha les picaba.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a71" ANNOTATION_REF="a34">
                <ANNOTATION_VALUE>Por eso, entonces, acabaronn las flechas de los indígenas. Ahí, entonces, le dejaron, todos</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a72" ANNOTATION_REF="a35">
                <ANNOTATION_VALUE>ya no les mataban a los yaguas, nada. Después, ya vivían tranquilo. En la cabecera de Pebas se murió Ruweju.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a73" ANNOTATION_REF="a36">
                <ANNOTATION_VALUE>Con una lanza le picaron en el poto. Así lo mataron; [la lanza] le destrozo sus intestinos.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a74" ANNOTATION_REF="a37">
                <ANNOTATION_VALUE>Todos se fueron a mirarle, entonces, eso les dijo a todos. Eso es todo que les cuento a ustedes señores. Muchas gracias.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER LINGUISTIC_TYPE_REF="Traducción" PARENT_REF="Traducción"
        PARTICIPANT="Manuel Santana" TIER_ID="Notas">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a76" ANNOTATION_REF="a42">
                <ANNOTATION_VALUE>¹ Es decir, los bebés sobrevivieron y crecieron. Cuando crecieron, le preguntaban a su abuela sobre qué había pasado con sus padres.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
