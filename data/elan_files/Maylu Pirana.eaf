    <TIER ANNOTATOR="Adriano Ingunza" LINGUISTIC_TYPE_REF="default-lt"
        PARTICIPANT="Rittma Urquía" TIER_ID="Transcripción">
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1"
                TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE>Muchikaygima maylunni yinkakgimananitkani.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2"
                TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE>Wanna gapka gimnitkana kapawalenni.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3"
                TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>Yagini pagoni gimatka, xripigima wa chowgu mtianatkanu wagipawna.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9"
                TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>Waneklu gima maylunitka wagerotgima nikanatanatkana gi gima wa gipnachri  jimkaluru nikanatatkana wa xripi gimni wa chowgumtyana wa chichmero. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a15"
                TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>Giyagnimatkani wa xripitka satu wa gagmuna tsru gagmungitsrukananu wa giprigachrinatka wane gima wa tkaspukmeromtlu wa chichmero, wale gima gakpa gatka.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a19"
                TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>Wa pagonimatkani satu kgiyolikaklergima wa giyolika jemgimamtlu wa chichmero.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a21"
                TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE> Chijjj chij chijj chi chijj chingimanata.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a27"
                TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE>Ya klunernige chingimata wane gima yatka rontapgimata. Kakmrukmerlegimatanaralga gimatana. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a28"
                TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE>Gaa tyetkani ga chijj chingimata.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a33"
                TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE>Chichi ktani chingimata gimakgimakni china gimatyawaka wale gimaki giwakatlu chichi. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a38"
                TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE>Chichmero giyagni chichmeroktani chingimata.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a42"
                TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE>Giyagni satokatka wale gima gike gimatka wa waletka wa ruyolikgimatatka klujta mchira gimni chkotungimni wa rutjita. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a46"
                TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE>Koshpagimatlo wa mchiranshigima rustaka tonshi gima yagotjimlegimatlo.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a47"
                TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE>Wane gima rawyatkalu nikgimatlu ponikotaa rixa ga kigleruktani watye chichi nanikanutkalu nomole nyegi chingimata ranikmergimatatkalu. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a51"
                TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE>Rutspegimtapana ramtaklewgimamtanatnaka walegimako satokanatatnaka.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a52"
                TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE>Wane gima rawapmerotatnaka jemgimatatnaklu chichi chichi chichi chichjjj. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a54"
                TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE>Rasukgimananumtatnaka chichi chichi chichi ralgimamtaatnaka rutspegimamtaatnakara gimamtyatnakgigima. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a57"
                TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE>Wale gimako satokanatatnaka kayitgimatanatka.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a58"
                TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE>Giyangimi wane gima china gaa giwanepotunanni giwa tyekatpje nwapanutkalu chingimata. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a59"
                TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE>Wale gimajo gwapatnaklu chicmero raskujimamta sato kgimatatnaka chichi chichi chichi. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a60"
                TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE>Kaspuklewgimamtanatka wane gima retyatkana wugenene. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a61"
                TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE>Makgimakta ¡Gaa papá chichmerotka ranikapka chingimata! gaga wale giwakno rushigkapgimatkoxatkalu wale. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a62"
                TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
                <ANNOTATION_VALUE>Walekni chichmero chingimata giyagni wane gima yoshikyatkaluna.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a69"
                TIME_SLOT_REF1="ts47" TIME_SLOT_REF2="ts48">
                <ANNOTATION_VALUE>Wa yagotgimatatkaluna. 
</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a73"
                TIME_SLOT_REF1="ts49" TIME_SLOT_REF2="ts50">
                <ANNOTATION_VALUE>Wa satu kayinre gima wa jimatkana punikpotgimlu waneklu gima rumtatkaluna yine wa chichmero. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a74"
                TIME_SLOT_REF1="ts51" TIME_SLOT_REF2="ts52">
                <ANNOTATION_VALUE>Wanepje nshi nika walepje. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER ANNOTATOR="Adriano Ingunza"
        LINGUISTIC_TYPE_REF="association-lt" PARENT_REF="Transcripción"
        PARTICIPANT="Rittma Urquía" TIER_ID="Traducción">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a5" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>Antiguamente, los gallinazos eran humanos.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a6" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>Ellos primero conocieron el fuego.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a7" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>Un día, vino un lorito de pico negro a robarles sus candelas. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a10" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>Desde esa vez, los gallinazos comieron carne cruda, ya no asada. El lorito pedrillo es el que les robó su candela. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a16" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE>Entonces, el lorito pedrillo soltó la candela en un árbol grande, seco. Ahí soltó su candela y eso empezó a arder. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a20" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE>Un día, un mitayero se fue a cazar y escuchó la candela. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a24" ANNOTATION_REF="a21">
                <ANNOTATION_VALUE>Sonaba chijjj, chij, chij.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a30" ANNOTATION_REF="a27">
                <ANNOTATION_VALUE>“¿Qué será?”, dijo el hombre, quien se acercó a mirar. Tocó la candela y se quemó bien fuerte. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a31" ANNOTATION_REF="a28">
                <ANNOTATION_VALUE>“¡Ah! Esto es lo que suena así”, dijo. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a34" ANNOTATION_REF="a33">
                <ANNOTATION_VALUE>“Entonces, su nombre es chichi”, dijo el hombre. A lo que sonaba le puso el nombre de “chichi”. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a39" ANNOTATION_REF="a38">
                <ANNOTATION_VALUE>Entonces, el hombre dijo que eso era la candela. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a44" ANNOTATION_REF="a42">
                <ANNOTATION_VALUE>Luego, él regresó, pero a buscar presas, presas. Ya había matado maquisapas y monos. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a48" ANNOTATION_REF="a46">
                <ANNOTATION_VALUE>Entonces, sacó un pedazo de su rabo de la maquisapa para probar si se le podía cocinar. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a49" ANNOTATION_REF="a47">
                <ANNOTATION_VALUE>De ahí la cogió, la comió y la probó. Se dio cuenta que estaba muy rico. “Está muy bueno, ahora voy a llevar para mi familia” dijo. Así, se llevó la candela. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a53" ANNOTATION_REF="a51">
                <ANNOTATION_VALUE>Así, llevó la candela. Cuando estaba caminando, se tropezó y olvidó el nombre de la candela. Se paró para recogerla nuevamente. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a55" ANNOTATION_REF="a52">
                <ANNOTATION_VALUE>De nuevo, trajo la candela y escuchó “chichi, chichi, chjj”.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a56" ANNOTATION_REF="a54">
                <ANNOTATION_VALUE>Nuevamente, cuando regresaba corriendo y diciendo chichi, se cayó y se olvidó del nombre. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a63" ANNOTATION_REF="a57">
                <ANNOTATION_VALUE>Nuevamente, volvió. Ya se le había hecho tarde. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a64" ANNOTATION_REF="a58">
                <ANNOTATION_VALUE>Entonces dijo: “esta será la última vez que la voy a traer”. Eso dijo. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a65" ANNOTATION_REF="a59">
                <ANNOTATION_VALUE>Otra vez se fue a traer corriendo la candela. Regresó nuevamente diciendo “chichi, chichi, chichi”. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a66" ANNOTATION_REF="a60">
                <ANNOTATION_VALUE>Nuevamente, soltó la candela. Cuando se levantó, vió a sus hijos. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a67" ANNOTATION_REF="a61">
                <ANNOTATION_VALUE>Ellos sabían el nombre  de la candela. “¡Ahh, papá está trayendo candela!”, dijeron. “¡Ahh, ese es el nombre!”, dijo el papá recordándolo en ese momento. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a68" ANNOTATION_REF="a62">
                <ANNOTATION_VALUE>“Ese es el nombre”, dijo. Entonces, todos pudieron hacer una fogata. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a71" ANNOTATION_REF="a69">
                <ANNOTATION_VALUE>Así la probaron. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a75" ANNOTATION_REF="a73">
                <ANNOTATION_VALUE>Entonces, asaron una de las presas que trajo el hombre y se las comieron. Estaba muy rico. Desde ahí, los yine conocieron la candela. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a76" ANNOTATION_REF="a74">
                <ANNOTATION_VALUE>Eso no más me acuerdo. Gracias.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
